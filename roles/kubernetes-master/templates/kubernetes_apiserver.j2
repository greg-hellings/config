# The address on the local server to listen to.
KUBE_API_ADDRESS="--address={{ kubernetes_private_ip }}"

# The port on the local server to listen on.
KUBE_API_PORT="--port={{ kubernetes_server_port }}"

# Port kubelets listen on
KUBELET_PORT="--kubelet-port={{ kubelet_port }}"

# Address range to use for services
KUBE_SERVICE_ADDRESSES="--service-cluster-ip-range={{ kubernetes_service_ip }}"

# Add your own!
KUBE_API_ARGS=""
